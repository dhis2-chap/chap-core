name: naive_python

docker_env:
  image: python:3.10

#python_env: python_env.yaml

entry_points:
  train:
    parameters:
      train_data: str
      model: str
    command: "python train.py {train_data} {model}"
  predict:
    parameters:
      historic_data: str
      future_data: str
      model: str
      out_file: str
    command: "python  predict.py {model} {historic_data} {future_data} {out_file}"
